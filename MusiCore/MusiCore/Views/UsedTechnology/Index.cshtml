@model IEnumerable<MusiCore.TextModels.UsedTechnology>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Html.Partial("PartialViews/_PartialViewForCreateTextTop")


<table class="table">
        <tr>
            <th>
            </th>
            <th>
                Teknolojiler
            </th>
            <th></th>
        </tr>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <div class="check"></div>
                </td>
               
                <td>
                    @Html.DisplayFor(modelItem => item.TextTurkish) &nbsp <a asp-action="Details" asp-route-id="@item.Id">Görüntüle</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<p>
    <a asp-action="Create">Yeni Teknoloji Ekle</a>
</p>