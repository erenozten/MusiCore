@model IEnumerable<MusiCore.TextModels.LogOfEverything>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Html.Partial("PartialViews/_PartialViewForCreateTextTop")

<table class="table">
    <thead>
        <tr>
            <th>
            </th>
            <th>
                Log'lar
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <div class="check"></div>
                </td>
                <td>
                        @Html.DisplayFor(modelItem => item.TextTurkish) <span style="font-style: italic; color: #00bfff">@item.GetMonth </span> <a asp-action="Details" asp-route-id="@item.Id">Görüntüle</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<p>
    <a asp-action="Create">Yeni Log Ekle</a>
</p>